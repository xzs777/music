AA,EAAA,GAAA,CAAA,IADA;AAEA,kBAAA,MAAA,EAAA,EAFA;AAGA,kBAAA,GAAA,EAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,GAHA;AAIA,kBAAA,GAAA,EAAA,GAAA,CAAA,EAAA,CAAA,MAJA;AAKA,kBAAA,EAAA,EAAA,GAAA,CAAA;AALA,iBAAA;AAOA,gBAAA,GAAA,CAAA,EAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,iBAFA;AAGA,gBAAA,KAAA,CAAA,MAAA,GAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,gBAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,UAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,cAAA,EAAA,KAAA;;AAhBA;AAAA;;AAAA;AAkBA,gBAAA,MAAA,CAAA,QAAA,CAAA;AACA,kBAAA,OAAA,EAAA,eADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuBA;AA7BA,GAVA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,aAAA;AACA;AA3CA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"swiper\">\n      <el-carousel :interval=\"4000\" type=\"card\" height=\"160px\">\n        <el-carousel-item v-for=\"(item, i) in swiperData\" :key=\"i\">\n          <img :src=\"item.pic\" alt @click=\"getMusicUrl(item.song)\" />\n        </el-carousel-item>\n      </el-carousel>\n    </div>\n    <!-- 推荐歌单 -->\n    <div class=\"recommend\">\n      <m-recommend></m-recommend>\n    </div>\n    <!-- 最新音乐 -->\n    <div class=\"newMusic\">\n      <m-newmusic></m-newmusic>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Recommend from '../music/Recommend.vue'\nimport NewSongs from '../music/Newmusic.vue'\nexport default {\n  data() {\n    return {\n      swiperData: []\n    }\n  },\n  components: {\n    'm-recommend': Recommend,\n    'm-newmusic': NewSongs\n  },\n  methods: {\n    async getSwiperData() {\n      const { data: res } = await this.$request.get('banner?type=1')\n      this.swiperData = res.banners\n    },\n    // 获取音乐播放地址\n    async getMusicUrl(obj) {\n      if (obj !== null) {\n        const { data: res } = await this.$request.get(`/song/url?id=${obj.id}`)\n        let music = {}\n        music = {\n          title: obj.name,\n          artist: [],\n          src: res.data[0].url,\n          pic: obj.al.picUrl,\n          id: obj.id\n        }\n        obj.ar.forEach(item => {\n          music.artist.push(item.name)\n        })\n        music.artist = music.artist.join(' ')\n        window.sessionStorage.setItem('nowMusic', JSON.stringify(music))\n        this.$store.dispatch('dealAutoPlay', music)\n      } else {\n        this.$message({\n          message: '点我没用,我没有数据!!!',\n          type: 'warning'\n        })\n      }\n    }\n  },\n  created() {\n    this.getSwiperData()\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.swiper {\n  height: 190px;\n  width: 98%;\n  border-radius: 10px;\n  margin: 0 auto;\n}  \n#img {\n    width:100%;\n    height:100%;\n    border-radius:6px;\n  }\n\n</style>\n"],"sourceRoot":"src/components/home"}]}